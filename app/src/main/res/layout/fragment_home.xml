<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jayxu.com.carassist.UI.HomeFragment"
    android:orientation="vertical">

    <jayxu.com.carassist.UI.HomeHexagonView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/HomeHexagonView"
        android:layout_gravity="left|top"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Home_MoneySavedByGoingElectric"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Home_Carhealth"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Home_EquaivlentMPG"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Home_NearbyCharger"/>

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_wallet"
        android:id="@+id/home_money"/>

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_mpg"
        android:id="@+id/home_mpg"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_electricity2"
        android:id="@+id/home_chargeStation"/>
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_locate"
        android:id="@+id/home_health"/>

</FrameLayout>